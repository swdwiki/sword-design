{
  "name": "swd-design",
  "version": "1.0.0",
  "description": "@sword-design vue components",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm --filter @swd-design/playground dev",
    "story": "pnpm --filter @swd-design/playground storybook",
    "docs:dev": "pnpm --filter @swd-design/docs dev",
    "docs:build": "cross-env NODE_ENV=production pnpm build && pnpm --filter @swd-design/docs build",
    "test": "cross-env NODE_ENV=test pnpm --filter @swd-design/components test",
    "build": "run-s build-hooks build-components",
    "build-components": "cross-env NODE_ENV=production pnpm --filter swd-design build",
    "build-hooks": "cross-env NODE_ENV=production pnpm --filter @swd-design/hooks build",
    "build:dev": "run-s build-hooks:dev build-components:dev",
    "build-components:dev": "cross-env NODE_ENV=development pnpm --filter swd-design build:watch",
    "build-hooks:dev": "cross-env NODE_ENV=development pnpm --filter @swd-design/hooks build"
  },
  "keywords": [],
  "author": "swdwiki",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@popperjs/core": "^2.11.8",
    "@swd-design/components": "workspace:*",
    "@swd-design/constants": "workspace:*",
    "@swd-design/hooks": "workspace:*",
    "@swd-design/locale": "workspace:*",
    "@swd-design/theme": "workspace:*",
    "@swd-design/utils": "workspace:*",
    "@swd-design/vite-plugins": "workspace:*",
    "@types/lodash": "^4.17.12",
    "@vitepress-preview/component": "workspace:*",
    "async-validator": "^4.2.5",
    "lodash-es": "^4.17.21",
    "swd-design": "workspace:*",
    "vue": "^3.4.27"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.12",
    "@types/shelljs": "^0.8.15",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-v8": "^1.4.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.1",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.2.5",
    "rollup": "^4.18.0",
    "shelljs": "^0.8.5",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^1.8.27",
    "vue3-i18n": "^1.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swdwiki/sword-design.git"
  },
  "bugs": {
    "url": "https://github.com/swdwiki/sword-design/issues"
  },
  "homepage": "https://github.com/swdwiki/sword-design#readme"
}
